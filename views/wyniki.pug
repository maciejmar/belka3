script.
  var wynikiS = '';
  console.log('tu cookie - - - ', getCookie('hbelki'));
  wynikS = 'Parametry:' + '<br>' +
  'hBelki=' + getCookie('hbelki') + '<br>' +
  'szBelki=' + getCookie('szbelki') + '<br>' +
  'otulina=' + getCookie('otulina') + '<br>' +
  'fi=' + getCookie('fi') + '<br><br>' +
  'Obliczono:' + '<br>' +
  'mi=' + getCookie('mi') + '<br>' +
  'As1=' + getCookie('As1') + '<br>'; 
  console.log('wynikS - >', wynikS);
  $("#wyniki_3").html(wynikS);
  
  function getCookie(cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(';');
  for(var i = 0; i <ca.length; i++) {
  var c = ca[i];
  while (c.charAt(0) == ' ') {
  c = c.substring(1);
  }
  if (c.indexOf(name) == 0) {
  return c.substring(name.length, c.length);
  }
  }
  return "";
  }
  
  
